<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="OnBoardingTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#261C12</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="layout_constraintStart_toStartOf">parent</item>
        <item name="layout_constraintEnd_toEndOf">parent</item>
    </style>
    <style name="SignInPageTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#4D000000</item>
    </style>
    <style name="OtherOptionButtonTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:focusable">true</item>
        <item name="android:textColor">@drawable/selector_text_btn</item>
    </style>
    <style name="SignInPageEditTextStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:background">@drawable/selector_edit_text</item>
        <item name="android:drawablePadding">8dp</item>
        <item name="android:paddingStart">14dp</item>
        <item name="android:paddingEnd">14dp</item>
        <item name="layout_constraintHeight_percent">0.0568</item>
        <item name="layout_constraintWidth_percent">0.8750</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="OtherLoginButtonsView">
        <item name="android:layout_width">0dp</item>
        <item name="android:focusableInTouchMode"> true</item>
        <item name="layout_constraintVertical_bias">0.2408</item>
        <item name="layout_constraintWidth_percent">0.2461</item>
        <item name="layout_constraintTop_toBottomOf">@id/loginSubmitButton</item>
        <item name="layout_constraintBottom_toBottomOf">parent</item>
        <item name="android:background">@drawable/button_other_login</item>
    </style>
</resources>